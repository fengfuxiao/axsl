<!--pages/homelist/homelist.wxml-->
<view class="homelist">
  <!-- 标题开始 -->
  <view class="homelist-title">
    {{ listDetail.title }}
  </view>
  <!-- 标题结束 -->
  <!-- 图片开始 -->
  <view class="homelist-image">
    <image src="{{ listDetail.image }}" mode=""/>
  </view>
  <!-- 图片结束 -->
  <!-- 内容开始 -->
  <view class="homelist-content">
    <text>{{ listDetail.detail }}</text>
  </view>
  <!-- 内容结束 -->
</view>
